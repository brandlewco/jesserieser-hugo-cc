{
  "name": "sendit_hugo",
  "version": "1.0.0",
  "description": "Starter template for hugo and bookshop",
  "main": "index.js",
  "scripts": {
    "start:css": "npx tailwindcss -i ./assets/css/main.css -o ./assets/css/style.css -w",
    "start:hugo": "hugo server",
    "start": "run-p start:*",
    "dev": "run-p hugo-server bookshop",
    "init": "npx @bookshop/init@latest",
    "hugo-server": "hugo server -D --watch",
    "build:css": "npx tailwindcss -i ./assets/css/main.css -o ./assets/css/style.css -m",
    "build:hugo": "hugo --source ./ --destination public --baseURL /",
    "build": "run-s build:*",
    "bookshop": "npx @bookshop/browser@latest",
    "bookshop-update": "npx @bookshop/up@latest",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CloudCannon/sendit-hugo-template.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@bookshop/browser": "3.3.3",
    "@bookshop/generate": "3.3.3",
    "@bookshop/hugo-engine": "3.3.3",
    "@swup/body-class-plugin": "^1.0.2",
    "@swup/fade-theme": "^1.0.5",
    "@swup/ga-plugin": "^1.0.1",
    "@swup/gtm-plugin": "^1.0.2",
    "@swup/preload-plugin": "^1.0.3",
    "@swup/scripts-plugin": "^1.1.0",
    "@swup/scroll-plugin": "^1.0.6",
    "browser-sync": "^2.26.7",
    "flickity": "^2.2.1",
    "imagesloaded": "^4.1.4",
    "isotope-layout": "^3.0.6",
    "lazysizes": "^5.2.0",
    "midday.js": "^1.0.2",
    "photoswipe": "^4.1.3",
    "rellax": "^1.12.1",
    "sal.js": "^0.7.4",
    "scroll-lock": "^2.1.4",
    "smoothscroll-polyfill": "^0.4.4",
    "swup": "^2.0.9",
    "swup-gtag-plugin": "0.0.0",
    "tailwindcss-flexbox-order": "^1.1.0"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.x",
    "@tailwindcss/aspect-ratio": "^0.x",
    "@tailwindcss/forms": "^0.x",
    "@tailwindcss/line-clamp": "^0.x",
    "@tailwindcss/typography": "^0.5.x",
    "autoprefixer": "^10.4.13",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "tailwindcss": "^3.2.7"
  }
}
